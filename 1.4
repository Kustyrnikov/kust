def count_it(sequence):
  count_dict = {}

  for char in sequence:
    if char.isdigit():
      num = int(char)
      count_dict[num] = count_dict.get(num, 0) + 1

  top_three = dict(sorted(count_dict.items(), key=lambda item: item[1], reverse=True)[:3])

  return top_three

sequence = "1233210812634105634564571009357486345863456589"
result = count_it(sequence)
print(result)
