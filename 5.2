def process_numbers(arr):
    positive_sum = sum(x for x in arr if x > 0)
    
    min_val, max_val = min(arr), max(arr)
    min_index = arr.index(min_val)
    max_index = arr.index(max_val)
    
    start, end = sorted([min_index, max_index])
    product_between = 1
    
    if end - start > 1:
        for num in arr[start+1:end]:
            product_between *= num
    
    return positive_sum, product_between

numbers = [3, -1, 6, 10, -4, 5]
result = process_numbers(numbers)
print("Сумма положительных элементов:", result[0])
print("Произведение чисел расположенных между минимальным и максимальным элементами:", result[1])
